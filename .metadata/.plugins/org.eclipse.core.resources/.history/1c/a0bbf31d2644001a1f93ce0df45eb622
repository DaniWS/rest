	"type": "object",
	"title": "The Schema for several measurements of the same resource (simplified version)",
	"type": "object",
	"properties": {
		"resourceId": {
			"$id": "#/properties/resourceId",
			"type": "string""title":"The Simplified ResourceId",
			"default": "",
			"examples": [
				"airTemperatureSensor0012"
			]
		},
		"sequenceNumber": {
			"$id": "#/properties/sequenceNumber",
			"type": "integer",
			"title": "The Sequence Number",
			"default": 0,
			"examples": [
				123
			]
		},
		"observations": {
			"$id": "#/properties/observations",
			"type": "array",
			"minItems": 1,
			"title": "The Observations Schema",
			"items": {
				"$id": "#/properties/observations/items",
				"type": "object",
				"required": {
					"resultTime": {
						"$id": "#/properties/observations/items/properties/resultTime",
						"type": "integer",
						"title": "Result time",
						"default": 0,
						"examples": [
							1558086914
						]
					},
					"result": {
						"$id": "#/properties/result",
						"type": "object",
						"title": {
							"value": {
								"type": "number"
							}
						},
						"additionalProperties": false
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false,
	"required": [
		"resourceId",
		"sequence number",
		"observations"
	]
